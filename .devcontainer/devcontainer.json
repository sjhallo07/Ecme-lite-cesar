{
  "name": "React TS Devcontainer",
  "image": "mcr.microsoft.com/devcontainers/node:20",
  "postCreateCommand": "true",
  "customizations": {
    "vscode": {
      "extensions": ["ms-vscode.vscode-typescript-next", "esbenp.prettier-vscode", "dbaeumer.vscode-eslint"],
      "settings": { "files.autoSave": "onFocusChange", "editor.formatOnSave": true }
    }
  }
}
